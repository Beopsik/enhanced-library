{{>layout/header}}

<h1>책 대여</h1>
{{#id}}
<button type="button" class="btn btn-primary" id="btn-show" value="{{id}}">대여 정보</button>
{{/id}}
<div class="col-md-12">
    <table class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>책 일련번호</th>
            <th>제목</th>
            <th>작성자</th>
            <th>줄거리 </th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#IssueList}}
            <tr>
                <td>{{guestId}}</td>
                <td>{{title}}</a></td>
                <td>{{author}}</td>
                <td>{{issuePeriod}}</td>
            </tr>
        {{/IssueList}}
        </tbody>
    </table>
</div>
{{>layout/footer}}